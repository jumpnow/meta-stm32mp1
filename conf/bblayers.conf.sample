# POKY_BBLAYERS_CONF_VERSION is increased each time build/conf/bblayers.conf
# changes incompatibly
POKY_BBLAYERS_CONF_VERSION = "2"

BBPATH = "${TOPDIR}"
BBFILES ?= ""

BBLAYERS ?= " \
  ${HOME}/poky-nanbield/meta \
  ${HOME}/poky-nanbield/meta-poky \
  ${HOME}/poky-nanbield/meta-openembedded/meta-oe \
  ${HOME}/poky-nanbield/meta-openembedded/meta-networking \
  ${HOME}/poky-nanbield/meta-openembedded/meta-perl \
  ${HOME}/poky-nanbield/meta-openembedded/meta-python \
  ${HOME}/poky-nanbield/meta-security \
  ${HOME}/poky-nanbield/meta-st-stm32mp \
  ${HOME}/stm32mp1/meta-stm32mp1 \
"
